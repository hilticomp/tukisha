<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="171pt"
        android:layout_height="70pt"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true"
        android:layout_gravity="top"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/t_bunnerd" />

    <ImageView
        android:id="@+id/ivVirgin"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/ivMunicipality"
        android:layout_marginStart="11dp"
        app:srcCompat="@drawable/virgin_mobile" />

    <ImageView
        android:id="@+id/ivPrasa"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/ivTelkom"
        android:layout_marginBottom="28dp"
        app:srcCompat="@drawable/prasa" />

    <ImageView
        android:id="@+id/ivMunicipality"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_above="@+id/ivPrasa"
        android:layout_alignEnd="@+id/ivPrasa"
        android:layout_marginBottom="15dp"
        android:layout_marginEnd="14dp"
        app:srcCompat="@drawable/municipality" />

    <ImageView
        android:id="@+id/ivCellC"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/ivTelkom"
        app:srcCompat="@drawable/cell_c" />

    <ImageView
        android:id="@+id/ivTelkom"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_alignStart="@+id/ivVodacom"
        android:layout_alignTop="@+id/ivMunicipality"
        app:srcCompat="@drawable/telkom" />

    <ImageView
        android:id="@+id/ivEskom"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView20"
        android:layout_marginTop="27dp"
        app:srcCompat="@drawable/eskom" />

    <ImageView
        android:id="@+id/ivVodacom"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/ivEskom"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/vodacom" />

    <ImageView
        android:id="@+id/ivMTN"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/ivVodacom"
        app:srcCompat="@drawable/mtn_main_logo" />


</RelativeLayout>